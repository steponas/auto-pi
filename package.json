    "mysql": "^2.17.1",
    "node-dht-sensor": "^0.4.3",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "serialport": "^11.0.0",
    "styled-components": "^5.3.11"
  },
