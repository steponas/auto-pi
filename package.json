{
  "name": "home-relay",
  "version": "0.0.1",
  "description": "Home automation through I2C relays on Raspberry Pi",
  "main": "index.js",
  "author": "Steponas Dauginis <steponas.dauginis@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "stop-relays": "node ./dist/cli-stop-relays.js",
    "toggle": "node ./dist/cli-toggle.js",
    "temp": "node ./dist/cli-temp.js",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "test": "jest",
    "build": "rm -rf dist/* && yarn build:client && yarn build:server",
    "build:client": "webpack --config webpack/prod.babel.js",
    "build:server": "webpack --config webpack/server.js",
    "start": "yarn build && yarn server",
    "dev": "concurrently \"yarn dev:watch\" \"yarn server\"",
    "server": "node ./dist/server.js",
    "dev:watch": "webpack --config webpack/dev.babel.js --watch"
  },
  "dependencies": {
